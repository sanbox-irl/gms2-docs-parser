{
	"compilerOptions": {
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"strict": true,
		"alwaysStrict": true,
		"target": "es2018",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "out",
		"lib": [
			"es2018",
		],
		"preserveConstEnums": true,
		"sourceMap": true,
	},
}